<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="liveQuizMultipleChoicesVM" type="com.application.app.modules.livequizmultiplechoices.data.viewmodel.LiveQuizMultipleChoicesVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linear51LiveQuiz"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyledeep_purple_400">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_52pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearTop"
                android:layout_height="match_parent"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearParticipant"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.19"
                    android:layout_width="0dp"
                    android:orientation="horizontal" style="@style/groupStyledeep_purple_4">
                    <ImageView android:id="@+id/imageIcon"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_icon_45"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt1"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_weight="0.27"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{liveQuizMultipleChoicesVM.liveQuizMultipleChoicesModel.txt1}"
                        style="@style/txtRubikmedium12_4" tools:text="@string/lbl_1"/>
                </LinearLayout>
                <ProgressBar android:id="@+id/progressBarGroup260"
                    android:indeterminate="false"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_marginStart="@dimen/_39pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_weight="0.58"
                    android:layout_width="0dp" android:max="100"
                    android:minHeight="@dimen/_4pxv"
                    android:progress="30"
                    android:progressDrawable="@drawable/layer_list_bg_white_a700_6c_radius_24" style="@style/Widget.AppCompat.ProgressBar.Horizontal"/>
                <LinearLayout android:id="@+id/linearPoints"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_31pxh"
                    android:layout_weight="0.23"
                    android:layout_width="0dp"
                    android:orientation="horizontal" style="@style/groupStyledeep_orange_300cornerRadius">
                    <ImageView android:id="@+id/imageIcon1"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_icon_38"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt35"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_weight="0.5"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{liveQuizMultipleChoicesVM.liveQuizMultipleChoicesModel.txt35}"
                        style="@style/txtRubikmedium12_4" tools:text="@string/lbl_35"/>
                </LinearLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearContent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_8pxv"
            android:layout_marginEnd="@dimen/_8pxh"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_5">
            <FrameLayout android:id="@+id/frameGroup1834"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_64pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_24pxv" android:layout_width="@dimen/_64pxh">
                <FrameLayout android:background="@color/red_101"
                    android:id="@+id/frameGroup261"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                <TextView android:ellipsize="end" android:id="@+id/txt6"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{liveQuizMultipleChoicesVM.liveQuizMultipleChoicesModel.txt6}"
                    style="@style/txtRubikmedium24" tools:text="@string/lbl_6"/>
            </FrameLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearQuestion"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_32pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtQUESTION3OF1"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{liveQuizMultipleChoicesVM.liveQuizMultipleChoicesModel.txtQUESTION3OF1}"
                    style="@style/txtRubikmedium14_3" tools:text="@string/msg_question_3_of_1"/>
                <TextView android:id="@+id/txtWhichPlayerSc"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent"
                    android:text="@{liveQuizMultipleChoicesVM.liveQuizMultipleChoicesModel.txtWhichPlayerSc}"
                    style="@style/txtRubikmedium20" tools:text="@string/msg_which_player_sc"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearAnswers"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <TextView android:id="@+id/txtRobinVanPersi"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_16pxv"
                    android:paddingStart="@dimen/_24pxh"
                    android:paddingTop="@dimen/_16pxv"
                    android:text="@{liveQuizMultipleChoicesVM.liveQuizMultipleChoicesModel.txtRobinVanPersi}"
                    style="@style/txtSolidRoundedOutline" tools:text="@string/msg_robin_van_persi"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_deep_purple_50_radius_20"
                    android:ellipsize="end"
                    android:hint="@string/lbl_sadio_mane"
                    android:id="@+id/etSadioMane"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_20pxv"
                    android:paddingStart="@dimen/_24pxh"
                    android:paddingTop="@dimen/_20pxv"
                    android:singleLine="true"
                    android:text="@={liveQuizMultipleChoicesVM.liveQuizMultipleChoicesModel.etSadioManeValue}"
                    android:textColorHint="@color/gray_900" style="@style/etSolidRoundedOutline_1"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_deep_purple_50_radius_20"
                    android:ellipsize="end"
                    android:hint="@string/lbl_harry_kane"
                    android:id="@+id/etHarryKane"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_20pxv"
                    android:paddingStart="@dimen/_24pxh"
                    android:paddingTop="@dimen/_20pxv"
                    android:singleLine="true"
                    android:text="@={liveQuizMultipleChoicesVM.liveQuizMultipleChoicesModel.etHarryKaneValue}"
                    android:textColorHint="@color/gray_900" style="@style/etSolidRoundedOutline_1"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_deep_purple_50_radius_20"
                    android:ellipsize="end"
                    android:hint="@string/msg_christian_bente"
                    android:id="@+id/etChristianBente"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_20pxv"
                    android:paddingStart="@dimen/_24pxh"
                    android:paddingTop="@dimen/_20pxv"
                    android:singleLine="true"
                    android:text="@={liveQuizMultipleChoicesVM.liveQuizMultipleChoicesModel.etChristianBenteValue}"
                    android:textColorHint="@color/gray_900" style="@style/etSolidRoundedOutline_1"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
