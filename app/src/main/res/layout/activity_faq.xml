<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="faqVM" type="com.application.app.modules.faq.data.viewmodel.FaqVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linear82FAQ"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyledeep_purple_50">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_60pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearHelpSupport"
                android:layout_height="match_parent"
                android:layout_marginEnd="@dimen/_87pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageIcon"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_icon_2"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtHelpAndSuppor"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_38pxh"
                    android:layout_weight="0.89"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{faqVM.faqModel.txtHelpAndSuppor}"
                    style="@style/txtRubikmedium24_1" tools:text="@string/msg_help_and_suppor"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <FrameLayout android:id="@+id/frameGroup448"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_692pxv"
            android:layout_marginEnd="@dimen/_8pxh"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_marginTop="@dimen/_24pxv" android:layout_width="@dimen/_359pxh">
            <ImageView android:id="@+id/imageBase"
                android:layout_gravity="start"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:scaleType="fitXY" android:src="@drawable/img_base"/>
            <LinearLayout android:id="@+id/linearContent"
                android:layout_gravity="top|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_40pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_40pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearTitle"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtIntroToQueezy"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{faqVM.faqModel.txtIntroToQueezy}"
                        style="@style/txtRubikmedium20" tools:text="@string/msg_intro_to_queezy"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtUpdated1Mo"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{faqVM.faqModel.txtUpdated1Mo}"
                        style="@style/txtRubikregular14" tools:text="@string/msg_updated_1_mo"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearParagraph"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:id="@+id/txtQueezyAppsOff"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:text="@{faqVM.faqModel.txtQueezyAppsOff}"
                        style="@style/txtRubikregular16" tools:text="@string/msg_queezy_apps_off"/>
                    <TextView android:id="@+id/txtWithQueezyYou"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="match_parent"
                        android:text="@{faqVM.faqModel.txtWithQueezyYou}"
                        style="@style/txtRubikregular16" tools:text="@string/msg_with_queezy_you"/>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameGroup450"
                    android:layout_height="@dimen/_180pxv"
                    android:layout_marginTop="@dimen/_32pxv" android:layout_width="@dimen/_327pxh">
                    <ImageView android:id="@+id/imageFredericperez"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_180pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_fredericperez}" tools:src="@drawable/img_fredericperez"/>
                    <LinearLayout android:id="@+id/linearGroup446"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="@dimen/_16pxh"
                        android:layout_marginVertical="@dimen/_16pxv"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <ImageView android:id="@+id/imageGroup203"
                            android:layout_gravity="end"
                            android:layout_height="@dimen/_48pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_width="@dimen/_48pxh"
                            android:scaleType="fitXY" android:src="@drawable/img_group203"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearGroup201"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_34pxv"
                            android:layout_width="match_parent"
                            android:orientation="horizontal" style="@style/groupStylegray_100_b2cornerRadius">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtWatchOn"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_8pxv"
                                android:layout_marginStart="@dimen/_8pxh"
                                android:layout_marginTop="@dimen/_8pxv"
                                android:layout_weight="0.48"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{faqVM.faqModel.txtWatchOn}"
                                style="@style/txtGraphikregular12" tools:text="@string/lbl_watch_on"/>
                            <ImageView
                                android:id="@+id/imageAkariconsyout"
                                android:layout_height="@dimen/_16pxh"
                                android:layout_marginBottom="@dimen/_8pxv"
                                android:layout_marginStart="@dimen/_4pxh"
                                android:layout_marginTop="@dimen/_8pxv"
                                android:layout_width="@dimen/_16pxh"
                                android:scaleType="fitXY" android:src="@drawable/img_akariconsyout"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtYouTube"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_11pxv"
                                android:layout_marginEnd="@dimen/_8pxh"
                                android:layout_marginStart="@dimen/_4pxh"
                                android:layout_marginTop="@dimen/_11pxv"
                                android:layout_weight="0.38"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{faqVM.faqModel.txtYouTube}"
                                style="@style/txtGraphikmedium10" tools:text="@string/lbl_youtube"/>
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
