<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="inviteFriendsVM" type="com.application.app.modules.invitefriends.data.viewmodel.InviteFriendsVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linear83InviteFrie"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyledeep_purple_400">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_60pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearInviteFriends"
                android:layout_height="match_parent"
                android:layout_marginEnd="@dimen/_108pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageIcon"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_icon"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtInviteFriends"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_60pxh"
                    android:layout_weight="0.87"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{inviteFriendsVM.inviteFriendsModel.txtInviteFriends}"
                    style="@style/txtRubikmedium24" tools:text="@string/lbl_invite_friends"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <FrameLayout android:id="@+id/frameGroup437"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_392pxv"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_114pxv" android:layout_width="@dimen/_327pxh">
            <FrameLayout android:id="@+id/frameGroup440"
                android:layout_gravity="start"
                android:layout_height="@dimen/_392pxv"
                android:layout_width="@dimen/_327pxh" style="@style/groupStyledeep_purple_300cornerRadius">
                <FrameLayout android:id="@+id/frameGroup442"
                    android:layout_gravity="top|center"
                    android:layout_height="@dimen/_140pxv"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_24pxh" android:layout_width="@dimen/_279pxh">
                    <ImageView android:id="@+id/imageGroup6"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_140pxh"
                        android:layout_marginEnd="@dimen/_40pxh"
                        android:layout_marginStart="@dimen/_40pxh"
                        android:layout_width="@dimen/_180pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_group6"/>
                    <ImageView android:id="@+id/imageAvatar"
                        android:layout_gravity="top|end"
                        android:layout_height="@dimen/_64pxh"
                        android:layout_marginBottom="@dimen/_24pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_24pxv"
                        android:layout_width="@dimen/_64pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_avatar"/>
                    <ImageView android:id="@+id/imageAvatar1"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_64pxh"
                        android:layout_marginBottom="@dimen/_24pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_24pxv"
                        android:layout_width="@dimen/_64pxh"
                        android:scaleType="fitXY" circular="@{true}"
                        imageRes="@{@drawable/img_avatar_1}" tools:src="@drawable/img_avatar_1"/>
                </FrameLayout>
                <LinearLayout android:id="@+id/linearGroup431"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_44pxv"
                    android:layout_marginEnd="@dimen/_34pxh"
                    android:layout_marginStart="@dimen/_34pxh"
                    android:layout_marginTop="@dimen/_44pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <FrameLayout android:id="@+id/frameGroup444"
                        android:layout_height="@dimen/_32pxv"
                        android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_43pxh">
                        <com.google.android.material.slider.RangeSlider
                            android:id="@+id/rangeBarGroup3"
                            android:layout_gravity="center"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_9pxh"
                            android:layout_marginStart="@dimen/_9pxh"
                            android:layout_width="match_parent"
                            android:valueFrom="0" android:valueTo="100"
                            app:labelBehavior="gone"
                            app:thumbColor="@color/bluegray_100"
                            app:thumbRadius="@dimen/_5pxh"
                            app:trackColorActive="@color/bluegray_100"
                            app:trackColorInactive="@color/bluegray_100" app:values="@array/range_slider_values"/>
                        <com.google.android.material.slider.RangeSlider
                            android:id="@+id/rangeBarGroup5"
                            android:layout_gravity="bottom|start"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="match_parent"
                            android:valueFrom="0" android:valueTo="100"
                            app:labelBehavior="gone"
                            app:thumbColor="@color/bluegray_100"
                            app:thumbRadius="@dimen/_5pxh"
                            app:trackColorActive="@color/bluegray_100"
                            app:trackColorInactive="@color/bluegray_100" app:values="@array/range_slider_values1"/>
                    </FrameLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtVS"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_64pxh"
                        android:layout_weight="0.5"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{inviteFriendsVM.inviteFriendsModel.txtVS}"
                        style="@style/txtRubikbold32" tools:text="@string/lbl_vs"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageUnion"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_264pxv"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY" android:src="@drawable/img_union"/>
            </FrameLayout>
            <LinearLayout android:id="@+id/linearGroup432"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_24pxh"
                android:layout_marginVertical="@dimen/_24pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <TextView android:id="@+id/txtInviteFriends1"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:text="@{inviteFriendsVM.inviteFriendsModel.txtInviteFriends1}"
                    style="@style/txtRubikmedium18" tools:text="@string/msg_invite_friends"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnQU123Z"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_16pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_16pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_qu123z"
                    android:textAllCaps="false"
                    style="@style/btnSolidRoundedOutline" tools:text="@string/lbl_qu123z"/>
                <LinearLayout android:id="@+id/linearButtons"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_32pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearButton"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.79"
                        android:layout_width="0dp"
                        android:orientation="horizontal" style="@style/groupStyledeep_purple_400cornerRadius">
                        <ImageView android:id="@+id/imageIcon1"
                            android:layout_height="@dimen/_24pxh"
                            android:layout_marginBottom="@dimen/_16pxv"
                            android:layout_marginStart="@dimen/_42pxh"
                            android:layout_marginTop="@dimen/_16pxv"
                            android:layout_width="@dimen/_24pxh"
                            android:scaleType="fitXY" android:src="@drawable/img_icon_1"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtCopyCode"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_16pxv"
                            android:layout_marginEnd="@dimen/_42pxh"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_16pxv"
                            android:layout_weight="0.77"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{inviteFriendsVM.inviteFriendsModel.txtCopyCode}"
                            style="@style/txtRubikmedium16" tools:text="@string/lbl_copy_code"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageShare"
                        android:layout_height="@dimen/_56pxh"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_width="@dimen/_56pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_share"/>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
