<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="settings1RowModel" type="com.application.app.modules.settings.data.model.Settings1RowModel"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearAccount"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="vertical">
        <TextView android:alpha="0.5" android:ellipsize="end"
            android:id="@+id/txtACCOUNT"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{settings1RowModel.txtACCOUNT}"
            style="@style/txtRubikmedium14" tools:text="@string/lbl_account"/>
        <LinearLayout android:id="@+id/linearGroup456"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStyledeep_purple_50cornerRadius">
            <ImageView android:id="@+id/imageIcon"
                android:layout_height="@dimen/_44pxh"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="@dimen/_44pxh"
                android:scaleType="fitXY" android:src="@drawable/img_icon_4"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearUpdateProfile"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_weight="0.76" android:layout_width="0dp" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtUpdateProfile"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{settings1RowModel.txtUpdateProfile}"
                    style="@style/txtRubikmedium16_1" tools:text="@string/lbl_update_profile"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtUpdateUsername"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{settings1RowModel.txtUpdateUsername}"
                    style="@style/txtRubikregular14" tools:text="@string/msg_update_username"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageIcon1"
                android:layout_height="@dimen/_20pxh"
                android:layout_marginBottom="@dimen/_30pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_26pxv"
                android:layout_width="@dimen/_20pxh"
                android:scaleType="fitXY" android:src="@drawable/img_icon_5"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearGroup457"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStyledeep_purple_50cornerRadius">
            <ImageView android:id="@+id/imageIcon2"
                android:layout_height="@dimen/_44pxh"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="@dimen/_44pxh"
                android:scaleType="fitXY" android:src="@drawable/img_icon_6"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearChangeEmailAd"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_weight="0.73" android:layout_width="0dp" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtChangeEmailAd"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{settings1RowModel.txtChangeEmailAd}"
                    style="@style/txtRubikmedium16_1" tools:text="@string/msg_change_email_ad"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMadiasyahooco"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{settings1RowModel.txtMadiasyahooco}"
                    style="@style/txtRubikregular14" tools:text="@string/msg_madias_yahoo_co"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageIcon3"
                android:layout_height="@dimen/_20pxh"
                android:layout_marginBottom="@dimen/_30pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_43pxh"
                android:layout_marginTop="@dimen/_26pxv"
                android:layout_width="@dimen/_20pxh"
                android:scaleType="fitXY" android:src="@drawable/img_icon_5"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearGroup458"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStyledeep_purple_50cornerRadius">
            <LinearLayout android:id="@+id/linearGroup463"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_weight="0.91" android:layout_width="0dp" android:orientation="horizontal">
                <ImageView android:id="@+id/imageIcon4"
                    android:layout_height="@dimen/_44pxh"
                    android:layout_width="@dimen/_44pxh"
                    android:scaleType="fitXY" android:src="@drawable/img_icon_7"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearChangePassword"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_weight="0.77"
                    android:layout_width="0dp" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtChangePassword"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_5pxh"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{settings1RowModel.txtChangePassword}"
                        style="@style/txtRubikmedium16_1" tools:text="@string/lbl_change_password"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLastChange1Y"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{settings1RowModel.txtLastChange1Y}"
                        style="@style/txtRubikregular14" tools:text="@string/msg_last_change_1_y"/>
                </LinearLayout>
            </LinearLayout>
            <ImageView android:id="@+id/imageIcon5"
                android:layout_height="@dimen/_20pxh"
                android:layout_marginBottom="@dimen/_30pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_71pxh"
                android:layout_marginTop="@dimen/_26pxv"
                android:layout_width="@dimen/_20pxh"
                android:scaleType="fitXY" android:src="@drawable/img_icon_5"/>
        </LinearLayout>
    </LinearLayout>
</layout>
