<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="liveQuizCheckboxesVM" type="com.application.app.modules.livequizcheckboxes.data.viewmodel.LiveQuizCheckboxesVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linear55LiveQuiz"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyledeep_purple_400">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_52pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearTop"
                android:layout_height="match_parent"
                android:layout_marginEnd="@dimen/_23pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearParticipant"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.19"
                    android:layout_width="0dp"
                    android:orientation="horizontal" style="@style/groupStyledeep_purple_4">
                    <ImageView android:id="@+id/imageIcon"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_icon_37"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt1"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_weight="0.27"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{liveQuizCheckboxesVM.liveQuizCheckboxesModel.txt1}"
                        style="@style/txtRubikmedium12_4" tools:text="@string/lbl_1"/>
                </LinearLayout>
                <ProgressBar android:id="@+id/progressBarGroup227"
                    android:indeterminate="false"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_marginStart="@dimen/_39pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_weight="0.57"
                    android:layout_width="0dp" android:max="100"
                    android:minHeight="@dimen/_4pxv"
                    android:progress="70"
                    android:progressDrawable="@drawable/layer_list_bg_white_a700_6c_radius_24" style="@style/Widget.AppCompat.ProgressBar.Horizontal"/>
                <LinearLayout android:id="@+id/linearPoints"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_weight="0.23"
                    android:layout_width="0dp"
                    android:orientation="horizontal" style="@style/groupStyledeep_orange_300cornerRadius">
                    <ImageView android:id="@+id/imageIcon1"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_icon_38"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt40"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_weight="0.5"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{liveQuizCheckboxesVM.liveQuizCheckboxesModel.txt40}"
                        style="@style/txtRubikmedium12_4" tools:text="@string/lbl_40"/>
                </LinearLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearContent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_8pxv"
            android:layout_marginEnd="@dimen/_8pxh"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_5">
            <FrameLayout android:id="@+id/frameGroup1811"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_64pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_24pxv" android:layout_width="@dimen/_64pxh">
                <FrameLayout android:background="@color/red_101"
                    android:id="@+id/frameGroup228"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                <TextView android:ellipsize="end" android:id="@+id/txt8"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{liveQuizCheckboxesVM.liveQuizCheckboxesModel.txt8}"
                    style="@style/txtRubikmedium24" tools:text="@string/lbl_8"/>
            </FrameLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearQuestion"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_32pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtQUESTION7OF1"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{liveQuizCheckboxesVM.liveQuizCheckboxesModel.txtQUESTION7OF1}"
                    style="@style/txtRubikmedium14_3" tools:text="@string/msg_question_7_of_1"/>
                <TextView android:id="@+id/txtWhichThreePla"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent"
                    android:text="@{liveQuizCheckboxesVM.liveQuizCheckboxesModel.txtWhichThreePla}"
                    style="@style/txtRubikmedium20" tools:text="@string/msg_which_three_pla"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearCheckboxAnswer"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_166pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearGroup1807"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStyledeep_purple_400_1">
                    <ImageView android:id="@+id/imageCheckbox"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="@dimen/_25pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_checkbox"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMohamedSalah"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginEnd="@dimen/_151pxh"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_weight="0.83"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{liveQuizCheckboxesVM.liveQuizCheckboxesModel.txtMohamedSalah}"
                        style="@style/txtRubikmedium16_1" tools:text="@string/lbl_mohamed_salah"/>
                </LinearLayout>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_deep_purple_50_radius_20"
                    android:drawablePadding="@dimen/_10pxh"
                    android:drawableStart="@drawable/img_checkbox_1"
                    android:ellipsize="end"
                    android:hint="@string/lbl_marcus_rashford"
                    android:id="@+id/etMarcusRashford"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_20pxv"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_20pxv"
                    android:singleLine="true"
                    android:text="@={liveQuizCheckboxesVM.liveQuizCheckboxesModel.etMarcusRashfordValue}"
                    android:textColorHint="@color/gray_900" style="@style/etSolidRoundedOutline_1"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearGroup1808"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStyledeep_purple_400_1">
                    <ImageView android:id="@+id/imageCheckbox1"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="@dimen/_25pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_checkbox"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSadioMane"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginEnd="@dimen/_184pxh"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_weight="0.78"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{liveQuizCheckboxesVM.liveQuizCheckboxesModel.txtSadioMane}"
                        style="@style/txtRubikmedium16_1" tools:text="@string/lbl_sadio_mane"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearGroup1809"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStyledeep_purple_400_1">
                    <ImageView android:id="@+id/imageCheckbox2"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="@dimen/_25pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_checkbox"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtAubameyang"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginEnd="@dimen/_171pxh"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_weight="0.8"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{liveQuizCheckboxesVM.liveQuizCheckboxesModel.txtAubameyang}"
                        style="@style/txtRubikmedium16_1" tools:text="@string/lbl_aubameyang"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
