<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="leaderboardAllTimeFull1VM" type="com.application.app.modules.leaderboardalltimefull1.data.viewmodel.LeaderboardAllTimeFull1VM"/>
    </data>
    <FrameLayout android:id="@+id/frameGroup2217"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStyledeep_purple_400">
        <LinearLayout android:id="@+id/linearGroup725"
            android:layout_gravity="bottom|center"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="wrap_content" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearLeaderboard"
                    android:layout_height="match_parent"
                    android:layout_marginEnd="@dimen/_90pxh"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageIcon"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_icon"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLeaderboard"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_66pxh"
                        android:layout_weight="0.86"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{leaderboardAllTimeFull1VM.leaderboardAllTimeFull1Model.txtLeaderboard}"
                        style="@style/txtGraphikmedium24_1" tools:text="@string/lbl_leaderboard"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <ScrollView android:clipToPadding="false"
                android:fillViewport="true"
                android:id="@+id/scrollViewScrollview"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_weight="1" android:layout_width="match_parent">
                <LinearLayout android:gravity="end"
                    android:id="@+id/linear73Leaderboard"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <com.google.android.material.tabs.TabLayout
                        android:id="@+id/tabLayoutGroup23"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_4pxh"
                        android:layout_marginStart="@dimen/_51pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="match_parent"
                        app:tabBackground="@drawable/selector_bg_deep_purple_300_radius_16"
                        app:tabIndicatorColor="@android:color/transparent"
                        app:tabIndicatorHeight="0dp" app:tabMode="fixed"
                        app:tabSelectedTextColor="@color/white_A700"
                        app:tabTextColor="@color/gray_300_99" style="@style/groupStylegray_900_75cornerRadius"/>
                    <androidx.viewpager2.widget.ViewPager2
                        android:gravity="start"
                        android:id="@+id/viewPagerGroup721"
                        android:layout_height="match_parent"
                        android:layout_marginTop="@dimen/_52pxv"
                        android:layout_weight="1" android:layout_width="match_parent"/>
                </LinearLayout>
            </ScrollView>
        </LinearLayout>
        <ImageView android:id="@+id/imageBase"
            android:layout_gravity="bottom|center"
            android:layout_height="1112dp"
            android:layout_marginEnd="@dimen/_8pxh"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:scaleType="fitXY" android:src="@drawable/img_base_6"/>
    </FrameLayout>
</layout>
