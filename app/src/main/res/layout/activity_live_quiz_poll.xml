<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="liveQuizPollVM" type="com.application.app.modules.livequizpoll.data.viewmodel.LiveQuizPollVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linear56LiveQuiz"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyledeep_purple_400">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_52pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearTop"
                android:layout_height="match_parent"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearParticipant"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.19"
                    android:layout_width="0dp"
                    android:orientation="horizontal" style="@style/groupStyledeep_purple_4">
                    <ImageView android:id="@+id/imageIcon"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_icon_37"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt1"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_weight="0.27"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{liveQuizPollVM.liveQuizPollModel.txt1}"
                        style="@style/txtRubikmedium12_4" tools:text="@string/lbl_1"/>
                </LinearLayout>
                <ProgressBar android:id="@+id/progressBarGroup223"
                    android:indeterminate="false"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_marginStart="@dimen/_39pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_weight="0.58"
                    android:layout_width="0dp" android:max="100"
                    android:minHeight="@dimen/_4pxv"
                    android:progress="80"
                    android:progressDrawable="@drawable/layer_list_bg_white_a700_6c_radius_24" style="@style/Widget.AppCompat.ProgressBar.Horizontal"/>
                <LinearLayout android:id="@+id/linearPoints"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_31pxh"
                    android:layout_weight="0.23"
                    android:layout_width="0dp"
                    android:orientation="horizontal" style="@style/groupStyledeep_orange_300cornerRadius">
                    <ImageView android:id="@+id/imageIcon1"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY" android:src="@drawable/img_icon_38"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt25"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_weight="0.48"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{liveQuizPollVM.liveQuizPollModel.txt25}"
                        style="@style/txtRubikmedium12_4" tools:text="@string/lbl_252"/>
                </LinearLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearContent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_8pxv"
            android:layout_marginEnd="@dimen/_8pxh"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_5">
            <FrameLayout android:id="@+id/frameGroup1805"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_64pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_24pxv" android:layout_width="@dimen/_64pxh">
                <FrameLayout android:background="@color/red_101"
                    android:id="@+id/frameGroup224"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content" android:layout_width="wrap_content"/>
                <TextView android:ellipsize="end" android:id="@+id/txt6"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{liveQuizPollVM.liveQuizPollModel.txt6}"
                    style="@style/txtRubikmedium24" tools:text="@string/lbl_6"/>
            </FrameLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearQuestion"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_32pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtQUESTION8OF1"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{liveQuizPollVM.liveQuizPollModel.txtQUESTION8OF1}"
                    style="@style/txtRubikmedium14_3" tools:text="@string/msg_question_8_of_1"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtWhatIsTheBes"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{liveQuizPollVM.liveQuizPollModel.txtWhatIsTheBes}"
                    style="@style/txtRubikmedium20" tools:text="@string/msg_what_is_the_bes"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearPollAnswers"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearGroup1803"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylewhite_6">
                    <TextView android:id="@+id/txtManchesterUnit"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.9"
                        android:layout_width="0dp"
                        android:paddingBottom="@dimen/_16pxv"
                        android:paddingStart="@dimen/_23pxh"
                        android:paddingTop="@dimen/_16pxv"
                        android:text="@{liveQuizPollVM.liveQuizPollModel.txtManchesterUnit}"
                        style="@style/txtSolidRounded" tools:text="@string/msg_manchester_unit"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt92"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_18pxv"
                        android:layout_marginEnd="@dimen/_15pxh"
                        android:layout_marginStart="@dimen/_19pxh"
                        android:layout_marginTop="@dimen/_18pxv"
                        android:layout_weight="0.1"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{liveQuizPollVM.liveQuizPollModel.txt92}"
                        style="@style/txtRubikmedium14_4" tools:text="@string/lbl_92"/>
                </LinearLayout>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_deep_purple_50_radius_20"
                    android:ellipsize="end"
                    android:hint="@string/lbl_leeds_united"
                    android:id="@+id/etLeedsUnited"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_20pxv"
                    android:paddingStart="@dimen/_23pxh"
                    android:paddingTop="@dimen/_20pxv"
                    android:singleLine="true"
                    android:text="@={liveQuizPollVM.liveQuizPollModel.etLeedsUnitedValue}"
                    android:textColorHint="@color/gray_900" style="@style/etSolidRoundedOutline_1"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_deep_purple_50_radius_20"
                    android:ellipsize="end"
                    android:hint="@string/lbl_fulham"
                    android:id="@+id/etFulham"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_20pxv"
                    android:paddingStart="@dimen/_23pxh"
                    android:paddingTop="@dimen/_20pxv"
                    android:singleLine="true"
                    android:text="@={liveQuizPollVM.liveQuizPollModel.etFulhamValue}"
                    android:textColorHint="@color/gray_900" style="@style/etSolidRoundedOutline_1"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_deep_purple_50_radius_20"
                    android:ellipsize="end"
                    android:hint="@string/lbl_leicester_city"
                    android:id="@+id/etLeicesterCity"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_20pxv"
                    android:paddingStart="@dimen/_23pxh"
                    android:paddingTop="@dimen/_20pxv"
                    android:singleLine="true"
                    android:text="@={liveQuizPollVM.liveQuizPollModel.etLeicesterCityValue}"
                    android:textColorHint="@color/gray_900" style="@style/etSolidRoundedOutline_1"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
