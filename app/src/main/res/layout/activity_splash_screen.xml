<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="splashScreenVM" type="com.application.app.modules.splashscreen.data.viewmodel.SplashScreenVM"/>
    </data>
    <FrameLayout android:id="@+id/frameGroup2211"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStyledeep_purple_400">
        <ImageView android:id="@+id/imageO"
            android:layout_gravity="bottom|start"
            android:layout_height="@dimen/_906pxv"
            android:layout_width="match_parent"
            android:scaleType="fitXY" android:src="@drawable/img_o_5"/>
        <LinearLayout android:id="@+id/linearAppLogo"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="@dimen/_40pxh"
            android:layout_marginVertical="@dimen/_40pxv"
            android:layout_width="wrap_content" android:orientation="vertical">
            <ImageView android:id="@+id/imageLogo"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_100pxh"
                android:layout_marginEnd="@dimen/_12pxh"
                android:layout_marginStart="@dimen/_12pxh"
                android:layout_width="@dimen/_100pxh"
                android:scaleType="fitXY" android:src="@drawable/img_logo_1"/>
            <TextView android:ellipsize="end" android:gravity="start"
                android:id="@+id/txtQueezy"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{splashScreenVM.splashScreenModel.txtQueezy}"
                style="@style/txtNunitoextrabold36" tools:text="@string/lbl_queezy"/>
        </LinearLayout>
    </FrameLayout>
</layout>
