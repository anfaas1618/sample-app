                    <TextView android:ellipsize="end"
                        android:id="@+id/txt2"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_18pxh"
                        android:layout_weight="0.1"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{quizCompletedVM.quizCompletedModel.txt2}"
                        style="@style/txtRubikmedium14_2" tools:text="@string/lbl_2"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt3"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_18pxh"
                        android:layout_weight="0.1"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{quizCompletedVM.quizCompletedModel.txt3}"
                        style="@style/txtRubikmedium14_2" tools:text="@string/lbl_3"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt4"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_18pxh"
                        android:layout_weight="0.1"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{quizCompletedVM.quizCompletedModel.txt4}"
                        style="@style/txtRubikmedium14_2" tools:text="@string/lbl_4"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt5"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_18pxh"
                        android:layout_weight="0.1"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{quizCompletedVM.quizCompletedModel.txt5}"
