                    <TextView android:ellipsize="end"
                        android:id="@+id/txt6"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_18pxh"
                        android:layout_weight="0.1"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{quizCompletedVM.quizCompletedModel.txt6}"
                        style="@style/txtRubikmedium14_2" tools:text="@string/lbl_6"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt7"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_18pxh"
                        android:layout_weight="0.1"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{quizCompletedVM.quizCompletedModel.txt7}"
                        style="@style/txtRubikmedium14_2" tools:text="@string/lbl_7"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt8"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_18pxh"
                        android:layout_weight="0.1"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{quizCompletedVM.quizCompletedModel.txt8}"
                        style="@style/txtRubikmedium14_2" tools:text="@string/lbl_8"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt9"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_18pxh"
                        android:layout_weight="0.1"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{quizCompletedVM.quizCompletedModel.txt9}"
                        style="@style/txtRubikmedium14_2" tools:text="@string/lbl_9"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt10"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_18pxh"
                        android:layout_weight="0.1"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{quizCompletedVM.quizCompletedModel.txt10}"
                        style="@style/txtRubikmedium14_2" tools:text="@string/lbl_10"/>
                </LinearLayout>
            </LinearLayout>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerGroup1792"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_6pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_16pxh}" tools:itemCount="2" tools:listitem="@layout/row_quiz_completed1"/>
